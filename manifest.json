{
  "name": "Imgur Client Redirect",
  "description": "Load a lightweight client for Imgur",
  "version": "1.1.4",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "48": "img/icon.svg",
    "128": "img/icon.svg"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://imgur.com/*",
    "*://www.imgur.com/*",
    "*://i.imgur.com/*"
  ]
}
